// Compute sum i for low <= i < high by recursion.
//
BEGF sum low high
    [13:0;2] BEGF T_BEGF 2 1 0 [$ MISSING $]; [< sum low high >]
    PUSH low
    [14:1;3] PUSHS T_PUSH 1 0 0 [$ MISSING $]; [< low * >]
    PUSH high
    [15:2;4] PUSHS T_PUSH 1 0 0 [$ MISSING $]; [< high * >]
    JMPGT EXIT
    [16:3;2] JMPGT T_JMPS 0 0 0 [$ MISSING $]; EXIT
    STACKS: VARIABLES: high low
            FUNCTIONS: | sum
    PUSH low
    [18:4;3] PUSHS T_PUSH 1 0 0 [$ MISSING $]; [< low * >]
    ADDI 1
    [19:5;3] ADDI T_AOP 0 0 0 1; *
    PUSH high
    [20:6;4] PUSHS T_PUSH 1 0 0 [$ MISSING $]; [< high * >]
    CALL sum 2 *
    [21:7;3] CALLM T_CALLM 2 1 0 [$ MISSING $]; [< sum * >]
    STACKS: VARIABLES: * high low
            FUNCTIONS: | sum
    PUSH low
    [23:8;4] PUSHS T_PUSH 2 0 0 [$ MISSING $]; [< low * >]
    STACKS: VARIABLES: * * high low
            FUNCTIONS: | sum
    ADD
    [25:9;3] ADD T_AOP 0 0 0 [$ MISSING $]; *
    RET 1
    [26:10;2] RET T_RET 0 1 1 [$ MISSING $]; [$ MISSING $]
    LABEL EXIT
    [16:3;2] JMPGT T_JMPS 0 0 8 [$ MISSING $]; EXIT
    PUSHI 0
    [28:11;3] PUSHI T_PUSH 0 0 0 0; *
    RET 1
    [29:12;2] RET T_RET 0 1 1 [$ MISSING $]; [$ MISSING $]
ENDF
    [13:0;2] BEGF T_BEGF 2 1 14 [$ MISSING $]; [< sum low high >]
    [30:13;0] ENDF T_ENDF 0 1 0 [$ MISSING $]; [$ MISSING $]

BEGF MAIN
    [32:14;0] BEGF T_BEGF 0 1 0 [$ MISSING $]; [< MAIN >]
    PUSHI 1 low
    [33:15;1] PUSHI T_PUSH 0 0 0 1; low
    PUSHI 1000000 high
    [34:16;2] PUSHI T_PUSH 0 0 0 1000000; high
    PUSH low
    [35:17;3] PUSHS T_PUSH 1 0 0 [$ MISSING $]; [< low * >]
    PUSH high
    [36:18;4] PUSHS T_PUSH 1 0 0 [$ MISSING $]; [< high * >]
    CALL sum 2 answer
    [37:19;3] CALLM T_CALLM 2 1 0 [$ MISSING $]; [< sum answer >]
    [39:20;3] PUSHS T_PUSH 2 0 0 [$ MISSING $]; [< low * >]
    [39:21;3] PUSHS T_PUSH 2 0 0 [$ MISSING $]; [< high * >]
    [39:22;3] PUSHS T_PUSH 2 0 0 [$ MISSING $]; [< answer * >]
    TRACE "answer = sum i: low <= i <= high" \
          low high answer
    [39:23;3] TRACE T_ALWAYS 3 0 0 [$ MISSING $]; [<
              "answer = sum i: low <= i <= high" low high answer >]
ENDF
    [32:14;3] BEGF T_BEGF 0 1 11 [$ MISSING $]; [< MAIN >]
    [40:24;0] ENDF T_ENDF 0 1 0 [$ MISSING $]; [$ MISSING $]
