// BEGF ... ENDF Test
// RESULT = MIN ( ARG1, ARG2 )
//
BEGF MIN ARG1 ARG2
    STACKS
    PUSH ARG1
    PUSH ARG2
    JMPLT ITS-ARG1
    PUSH ARG2
    RET 1
    LABEL ITS-ARG1
    PUSH ARG1
    RET 1
    STACKS
ENDF

PUSHI 23 X
PUSHI 157 Y
CALL MIN 2 Z
STACKS
POP

// BEGF ... ENDF and JMPTRUE Test
// RESULT = 1 IF ARG == TRUE AND 0 IF ARG == FALSE
//
BEGF FLIP ARG
    PUSH ARG
    JMPTRUE * ITS-0
    PUSH ARG
    JMPTRUE ITS-1
    PUSHI 555
    RET 1
    LABEL ITS-0
    PUSHI 1
    RET 1
    LABEL ITS-1
    PUSHI 0
    RET 1
ENDF

PUSHI "FALSE"
CALL FLIP 1 Z
POP
PUSHI "TRUE"
CALL FLIP 1 Z
POP
